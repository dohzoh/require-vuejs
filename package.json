  "description": "SystemJS plugin to async and dynamic load and parse .vue components in the browser without webpack",
  "main": "dist/plugin-vue.js",
  "author": "Yohzoh Matsui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dohzoh/systemjs-vue-inbrowser/pulls"
  },
  "homepage": "",
